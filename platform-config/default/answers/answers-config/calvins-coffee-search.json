{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "calvins-coffee-search",
  "name": "Calvin\u0027s Coffee Search",
  "supportedLocales": [
    "en",
    "de"
  ],
  "countryRestrictions": [],
  "verticals": {
    "faqs": {
      "entityTypes": [
        "faq"
      ],
      "name": "FAQs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "keywords": {
          "textSearch": true
        },
        "question": {
          "semanticTextSearch": true
        }
      },
      "sortBys": [],
      "source": "YEXT"
    }
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "popularQueriesBlacklistedRegex": [],
    "universalPrompts": [],
    "verticalPrompts": {
      "faqs": [
        "[[name]]"
      ]
    }
  }
}